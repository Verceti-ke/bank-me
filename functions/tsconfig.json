{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    // Turn this back on
    "noUnusedLocals": false,
    "outDir": "build",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "types": ["facebook-js-sdk"]
  },
  "compileOnSave": true,
  "include": [
    "src",
    "src/config/*.json"
  ]
}
