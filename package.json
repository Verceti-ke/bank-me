{
  "name": "bank-me",
  "version": "0.1.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build && node app.js",
    "nodemon": "nodemon",
    "lint": "ng lint",
    "run": "node app.js",
    "postinstall": "ng build --prod && npm install --prefix server",
    "local:start": "ng serve --port 4200 --configuration=local",
    "dev:start": "ng serve --port 4200",
    "build": "ng build --aot --optimization --build-optimizer",
    "build:ci": "if test \"$BRANCH_NAME\" = \"master\"; then npm run build -- --prod; else npm run build -- --prod; fi",
    "deploy": "firebase deploy --except functions --token \"$FIREBASE_TOKEN\" --non-interactive",
    "deploy:ci": "if test \"$BRANCH_NAME\" = \"master\"; then npm run deploy; fi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.8",
    "@angular/common": "^8.2.8",
    "@angular/compiler": "^8.2.8",
    "@angular/core": "^8.2.8",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^8.2.8",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.8",
    "@angular/platform-browser-dynamic": "^8.2.8",
    "@angular/router": "^8.2.8",
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "angular-2-dropdown-multiselect": "^1.9.0",
    "angular-tabs-component": "^1.0.6",
    "angular-webstorage-service": "^1.0.2",
    "angular2-fontawesome": "^5.2.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "firebase": "^5.8.2",
    "firebase-admin": "^5.13.1",
    "font-awesome": "^4.7.0",
    "moment": "^2.24.0",
    "ngx-moment": "^3.3.0",
    "ngx-toastr": "^11.0.0",
    "rxjs": "6.0.0",
    "rxjs-compat": "6.0.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.6",
